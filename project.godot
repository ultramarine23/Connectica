; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Connectica"
run/main_scene="res://BattleScene/battle_scene.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Consts="*res://Singletons/SNG_global_consts.gd"
Signals="*res://Singletons/SNG_global_signals.gd"
BattleInfo="*res://Singletons/SNG_battle_info.gd"
Managers="*res://Singletons/SNG_managers_registry.gd"
SFXBus="*res://Singletons/SNG_sfx_bus.tscn"

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/stretch/mode="canvas_items"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
